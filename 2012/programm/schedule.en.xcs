  <status>CONFIRMED</status>
  <category>Lecture</category>
  <url>http://www.fossgis.de/konferenz/2012/programm/events/369.en.html</url>
  <location>H&#246;rsaal 08</location>
  <attendee>Torsten Rahn</attendee>
</vevent>
