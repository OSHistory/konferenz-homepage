  <status>CONFIRMED</status>
  <category>Lecture</category>
  <url>http://www.fossgis.de/konferenz/2012/programm/events/371.en.html</url>
  <location>H&#246;rsaal 04/113</location>
  <attendee>Dominik Helle</attendee>
