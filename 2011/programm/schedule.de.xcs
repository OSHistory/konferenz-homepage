  <status>CONFIRMED</status>
  <category>Lecture</category>
  <url>http://www.fossgis.de/konferenz/2011/programm/events/309.en.html</url>
  <location>Gro&#223;er H&#246;rsaal Geb. 308</location>
  <attendee>Arnulf Christl</attendee>
</vevent>
