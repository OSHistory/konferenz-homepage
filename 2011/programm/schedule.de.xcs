  <status>CONFIRMED</status>
  <category>Lecture</category>
  <url>http://www.fossgis.de/konferenz/2011/programm/events/224.en.html</url>
  <location>H&#246;rsaal Geb. 227</location>
  <attendee>Marc Jansen</attendee>
  <attendee>Till Adams</attendee>
</vevent>
