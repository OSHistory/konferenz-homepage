  <status>CONFIRMED</status>
  <category>Lecture</category>
  <url>http://www.fossgis.de/konferenz/2013/programm/events/541.en.html</url>
  <location>3.008</location>
  <attendee>Stefan Oderbolz</attendee>
</vevent>
